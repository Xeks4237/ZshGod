# Function for showing arrow with current working directory
function prompt_zshgod_rectangular_current-pwd() {
	# Allow passing args to function for changing foreground and background colors
	local bg_color=$1
	local fg_color=$2

	PROMPT_ZSHGOD_RECTANGULAR_CURRENT_PWD="%F{$fg_color}%K{$bg_color} %~ %k%f"
}
