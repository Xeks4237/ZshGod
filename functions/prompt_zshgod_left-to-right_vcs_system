# Function which returns name of current vcs
function prompt_zshgod_left-to-right_vcs_system() {
	# Allow passing args to function for changing foreground and background colors
	local bg_color=$1
	local fg_color=$2

	[[ -n $(prompt_zshgod_vcs_system) ]] || return
	PROMPT_ZSHGOD_LEFT_TO_RIGHT_VCS_SYSTEM="%F{$bg_color}%S %K{$fg_color}$(prompt_zshgod_vcs_system)%k %s%f"
}
