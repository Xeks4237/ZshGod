# Function which shows colored arrow with different color if current user is root
function prompt_zshgod_right-to-left_root-indicator () {
	print "%(!,%F{$ZSHGOD_THEME_CATPPUCCIN_RED}%S %K{$ZSHGOD_THEME_CATPPUCCIN_CRUST}#%k %s%f,%F{$ZSHGOD_THEME_CATPPUCCIN_GREEN}%S %K{$ZSHGOD_THEME_CATPPUCCIN_CRUST}\$%k %s%f)"
}
