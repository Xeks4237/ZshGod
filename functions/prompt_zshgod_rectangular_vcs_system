# Function which returns name of current vcs
function prompt_zshgod_rectangular_vcs_system() {
	# Allow passing args to function for changing foreground and background colors
	local bg_color=$1
	local fg_color=$2

	[[ -n $(prompt_zshgod_vcs_system) ]] || return
	PROMPT_ZSHGOD_RECTANGULAR_VCS_SYSTEM="%F{$fg_color}%K{$bg_color} $(prompt_zshgod_vcs_system) %k%f"
}
