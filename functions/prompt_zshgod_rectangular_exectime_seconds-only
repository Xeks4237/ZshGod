# Function which returns exectime for commands after it's been called
function prompt_zshgod_rectangular_exectime_seconds-only() {
	# Check if exectime isn't empty and output exectime
	[[ -n $(prompt_zshgod_exectime) ]] && print "%F{$ZSHGOD_THM_CATPPUCCIN_CRUST}%K{$ZSHGOD_THM_CATPPUCCIN_YELLOW} $(prompt_zshgod_exectime)s %f%k"
}
