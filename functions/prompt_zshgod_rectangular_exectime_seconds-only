# Function which returns exectime for commands after it's been called
function prompt_zshgod_rectangular_exectime_seconds-only() {
	# Allow passing args to function for changing foreground and background colors
	local bg_color=$1
	local fg_color=$2

	# Check if exectime isn't empty and output exectime
	[[ -n $(prompt_zshgod_exectime) ]] && PROMPT_ZSHGOD_RECTANGULAR_EXECTIME_SECONDS_ONLY="%F{$fg_color}%K{$bg_color} $(prompt_zshgod_exectime)s %f%k"
}
