# Function which returns colored arrow with current username with different color if current user is root
function prompt_zshgod_right-to-left_username() {
	print "%(!,%F{$ZSHGOD_THM_CATPPUCCIN_RED}%S %K{$ZSHGOD_THM_CATPPUCCIN_CRUST}%n%k %s%f,%F{$ZSHGOD_THM_CATPPUCCIN_GREEN}%S %K{$ZSHGOD_THM_CATPPUCCIN_CRUST}%n%k %s%f)"
}
