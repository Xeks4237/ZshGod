# Function for showing arrow with customly formatted current time
function prompt_zshgod_right-to-left_time() {
	# Allow passing args to function for changing foreground and background colors
	local bg_color=$1
	local fg_color=$2

	PROMPT_ZSHGOD_RIGHT_TO_LEFT_TIME="%F{$bg_color}%S %K{$fg_color}%D{%H:%M:%S}%k %s%f"
}
